<%- model_class = Trial -%>

<div class="row">
  <div class='col-md-12'>
    <h1 class="page-header">Vencimientos Proximos</h1>
  </div>
</div>
<div class='row'>
<div class='col-md-12'>
  <% if @trials.blank? %>
<p>No hay juicios con vencimientos proximos</p>
<% end %>
</div>
<div class='col-md-12'>
  <%= render 'quick_table', model_class: model_class%>
</div>

<% if @trials.blank? %>
<div>
  <p><strong>No se encontraron Juicios</strong></p>
</div>
<% end %>
</div>
