<div class='row'>
  <div class="col-md-12 text-center">
    <h1>Iniciar Sesi√≥n</h1>
  </div>
</div>
<div class='row'>
  <%= form_for(:session, url: login_path) do |f| %>
    <div class='col-md-12'>
      <div class="col-md-4">
      </div>
      <div class='col-md-4'>
        <br><br>
        <%= f.label :username %>
        <%= f.text_field :username, class: 'form-control' %>
      </div>
      <div class="col-md-4">
      </div>
    </div>
    <div class='col-md-12'>
      <div class="col-md-4">
      </div>
      <div class="col-md-4">
        <br><br>
        <%= f.label :password %>
        <%= f.password_field :password, class: 'form-control' %>
      </div>
      <div class="col-md-4">
      </div>
    </div>
    <div class='col-md-12'>
      <div class="col-md-4">
      </div>
      <div class="col-md-4 text-center">
        <br><br>
        <%= f.submit "Log in", class: "btn btn-primary" %>
      </div>
      <div class="col-md-4">
      </div>
    </div>
  <% end %>
</div>
